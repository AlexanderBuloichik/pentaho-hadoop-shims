    <packaging>pom</packaging>
    <dependencies>
        <dependency>
            <groupId>pentaho</groupId>
            <artifactId>hadoop2-windows-patch</artifactId>
            <version>${hadoop2-windows-patch.version}</version>
        </dependency>
        <dependency>
            <groupId>pentaho</groupId>
